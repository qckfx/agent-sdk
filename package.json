{
  "name": "@qckfx/agent",
  "version": "0.2.0",
  "type": "module",
  "description": "qckfx AI Agent SDK",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "bin": {
    "validate": "./dist/cjs/cli/validate-config.js"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./browser": {
      "types": "./dist/esm/browser.d.ts",
      "import": "./dist/esm/browser.js",
      "require": "./dist/cjs/browser.cjs"
    },
    "./internals": {
      "types": "./dist/esm/internals.d.ts",
      "import": "./dist/esm/internals.js",
      "require": "./dist/cjs/internals.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/esm/index.d.ts"
      ]
    },
    "internals": {
      "types": [
        "dist/esm/internals.d.ts"
      ]
    },
    "browser": {
      "types": [
        "dist/esm/browser.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "schemas/agent-config.schema.json",
    "docker",
    "scripts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node postbuild-cjs.cjs",
    "build": "npm run build:esm && npm run build:cjs",
    "eval": "ts-node src/eval/index.ts",
    "eval:quick": "ts-node src/eval/index.ts run --quick --concurrency 4",
    "eval:custom": "ts-node src/eval/index.ts run --config",
    "eval:list": "ts-node src/eval/index.ts list",
    "eval:build": "npm run build && node dist/eval/index.js",
    "test": "vitest run",
    "docs": "typedoc",
    "docker:fix": "./scripts/fix-docker-mapper.sh",
    "docker:build": "docker build -t ghcr.io/earlywormteam/agent-sandbox:latest -f docker/Dockerfile .",
    "docker:publish": "npm run docker:build && docker push ghcr.io/earlywormteam/agent-sandbox:latest",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "sdk"
  ],
  "author": "qckfx",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.0.3",
    "e2b": "^1.2.5",
    "glob": "^11.0.1",
    "openai": "^4.96.0",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/prompts": "^2.4.9",
    "typedoc": "^0.28.2",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
